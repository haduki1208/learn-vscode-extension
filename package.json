{
  "name": "helloworld-ext",
  "displayName": "helloworld-ext",
  "description": "vscode extensionの勉強",
  "version": "0.0.1",
  "publisher": "haskey-app",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onView:my-activitybar",
    "onView:my-tree",
    "onView:my-webview"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "my-activitybar",
          "title": "オレオレactivitybar",
          "icon": "media/book.svg"
        }
      ]
    },
    "views": {
      "my-activitybar": [
        {
          "id": "my-tree",
          "name": "オレオレtree-view",
          "type": "tree"
        },
        {
          "id": "my-webview",
          "name": "オレオレwebview",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.54.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.19.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
